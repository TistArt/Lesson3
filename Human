package Les3;

public class Human {

private int age;
private boolean gender;
private String surname;

public Human(int age, boolean gender, String surname) {
	super();
	this.age = age;
	this.gender = gender;
	this.surname = surname;
}

public Human() {
	super();
}

public int getAge() {
	return age;
}

public void setAge(int age) {
	this.age = age;
}

public boolean isGender() {
	return gender;
}

public void setGender(boolean gender) {
	this.gender = gender;
}

public String getSurname() {
	return surname;
}

public void setSurname(String surname) {
	this.surname = surname;
}

@Override
public String toString() {
	return "Human [age=" + age + ", gender=" + gender + ", surname=" + surname + "]";
}

}
